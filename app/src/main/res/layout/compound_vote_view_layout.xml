<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <project.taras.ua.adrenalincity.Activity.MovieMVC.StarView.StarView
        android:id="@+id/compound_star_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/compound_b_vote_up"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/thumb_up" />

    <Button
        android:id="@+id/compound_b_vote_down"
        android:background="@drawable/thumb_down"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>